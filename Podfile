# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'Linner' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Linner

 pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'FirebaseUI/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  pod 'Alamofire', '~> 4.7'
  pod 'SwiftyJSON', '~> 4.0'
  pod 'GoogleSignIn'
  pod 'Firebase/Messaging'
  pod 'FBSDKLoginKit'
  pod 'Kingfisher', '~> 4.0'
  pod 'CountdownLabel'
  target 'LinnerTests' do
    inherit! :search_paths
    # Pods for testing
    
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
    pod 'FirebaseUI/Auth'
    pod 'Firebase/Database'
    pod 'Firebase/Storage'
    pod 'GoogleSignIn'
  end

  target 'LinnerUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
